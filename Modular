                                                                             Binary_Exponent+ Modular_Multiplicative_Inverse
 
 
 
 ll power(ll x,ll y,ll p)
    {
  ll res=1;
if(y==0)
return 1;
  x=x%p;
 if(x==0)
 return 0;
  while(y>0)
  {
    if(y&1)
    res=(res*x)%mod;
    y=y>>1;
    x=(x*x)%p;
  }
  return res;
}
